										}
									}
									use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0002]/items[at0005]/items[at0007]	-- /data[Arbol]/items[Ubicacion]/items[Via]/items[Tipo]
								}
							}
						}
					}
				}
			}
		}
	}

