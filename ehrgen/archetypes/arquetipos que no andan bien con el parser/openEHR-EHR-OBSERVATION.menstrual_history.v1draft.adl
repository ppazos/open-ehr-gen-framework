															>
														}
													}
													ELEMENT[at0011] occurrences matches {0..1} matches {	-- Minimum length of cycle
														value matches {
															C_DV_QUANTITY <
																property = <[openehr::128]>
																list = <
																	["1"] = <
																		units = <"a">
																		magnitude = <|>=0.0|>
																		precision = <|0|>
																	>
																	["2"] = <
																		units = <"mo">
																		magnitude = <|>=0.0|>
																		precision = <|0|>
																	>
																>
															>
														}
													}
													ELEMENT[at0012] occurrences matches {0..1} matches {	-- Maximum length of menstruation
														value matches {
															C_DV_QUANTITY <
																property = <[openehr::128]>
																list = <
