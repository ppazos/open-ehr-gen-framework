																		magnitude = <|>=0.0|>
																		precision = <|0|>
																	>
																	["2"] = <
																		units = <"wk">
																		magnitude = <|>=0.0|>
																		precision = <|0|>
																	>
																>
															>
														}
													}
													ELEMENT[at0013] occurrences matches {0..1} matches {	-- Minimum length of menstruation
														value matches {
